datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Referral {
  id        Int      @id @default(autoincrement())
  refereeEmail  String
  refereeName   String
  referrerName  String
  referrerEmail String
  createdAt DateTime @default(now())
}
